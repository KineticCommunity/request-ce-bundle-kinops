{
  "name": "request-ce-bundle-kinetic",
  "version": "2.1.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "cd packages/app && yarn start",
    "build": "cd packages/app && yarn build",
    "test": "cd packages/app && yarn test",
    "format": "prettier --write \"**\"",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "~1.13.3"
  },
  "lint-staged": {
    "**": [
      "prettier --write",
      "git add"
    ]
  }
}
